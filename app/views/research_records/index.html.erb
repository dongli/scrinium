<p id="notice"><%= notice %></p>

<h1>Listing Research Records</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @research_records.each do |research_record| %>
      <tr>
        <td><%= link_to 'Show', research_record %></td>
        <td><%= link_to 'Edit', edit_research_record_path(research_record) %></td>
        <td><%= link_to 'Destroy', research_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Research record', new_research_record_path %>
